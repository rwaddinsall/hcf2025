{
  "name": "hopkins-creek-festival",
  "description": "Hopkins Creek Music Festival website built with Astro and accessible-astro-components",
  "version": "1.0.0",
  "author": "Hopkins Creek Festival",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "fetch-content": "node scripts/fetch-strapi-content.js",
    "dev": "npm run fetch-content && astro dev",
    "start": "astro dev",
    "build": "npm run fetch-content && astro build",
    "build:check": "astro check",
    "preview": "astro preview",
    "format": "prettier --write \"**/*.{astro,js,ts,json,scss,css}\"",
    "lint": "eslint . --ext .js,.ts,.astro",
    "lint:fix": "eslint . --ext .js,.ts,.astro --fix",
    "format:check": "prettier --check \"**/*.{astro,js,ts,json,scss,css}\"",
    "deploy:prepare": "./scripts/deploy.sh",
    "deploy:build": "npm run format && npm run lint:fix && npm run build",
    "deploy:preview": "npm run deploy:build && npm run preview"
  },
  "keywords": [
    "astro",
    "music-festival",
    "hopkins-creek",
    "accessible",
    "accessibility",
    "a11y",
    "wcag",
    "responsive",
    "festival-website"
  ],
  "devDependencies": {
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/partytown": "^2.1.4",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@iconify-json/lucide": "^1.2.44",
    "@netlify/functions": "^2.8.2",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "astro": "^5.10.1",
    "astro-compress": "^2.3.8",
    "astro-eslint-parser": "^1.2.2",
    "astro-icon": "^1.1.5",
    "eslint": "^9.27.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "flyonui": "^2.2.0",
    "globals": "^16.1.0",
    "prettier": "^3.6.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-css-order": "^2.1.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sanitize-html": "^2.17.0",
    "sass": "^1.89.0",
    "svgo": "^3.3.2",
    "tailwindcss": "^4.1.7"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.4.0",
    "@astrojs/react": "^4.3.0",
    "@heroui/navbar": "^2.2.19",
    "@heroui/react": "^2.7.10",
    "@heroui/system": "^2.4.17",
    "@heroui/theme": "^2.4.17",
    "@iconify-json/mdi": "^1.2.3",
    "@strapi/blocks-react-renderer": "^1.0.2",
    "@strapi/client": "^1.4.0",
    "@tailwindcss/vite": "^4.1.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "accessible-astro-components": "^4.1.1",
    "framer-motion": "^12.18.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3"
  }
}
